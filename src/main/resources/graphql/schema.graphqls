schema{
    query: Query
}
type Query{
    getAll: [Product]
    getById(idProduct:Int): Product
}
type Product {
  idProduct: ID!
  comercialName: String!
  price: Float!
  stock: Int!
  servingSize: Int!
  unitServingSize: String!
  servings: Int!
  netContent: Int!
  unitNetContent: Int!
  presentation: String!
  description: String!
  caducity: String!
  lote: String!
  flavor: String!
  productRecomendation: String!
  imgProductPath: String!

}

